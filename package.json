{
	"name": "app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dock": "docker-compose up -d; docker exec -it nextcloud bash; docker-compose down",
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"buildopen": "npx prisma generate && opennextjs-cloudflare build",
		"deploy": "opennextjs-cloudflare deploy",
		"upload": "opennextjs-cloudflare upload",
		"preview": "opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"postinstall": "prisma generate",
		"generate": "dotenv -e .dev.vars -- npx prisma generate --no-engine"
	},
	"dependencies": {
		"@neondatabase/serverless": "^1.0.2",
		"@opennextjs/cloudflare": "^1.14.4",
		"@prisma/adapter-neon": "6.16.0",
		"@prisma/adapter-pg": "^6.16.0",
		"@prisma/client": "6.16.0",
		"@prisma/extension-accelerate": "^2.0.2",
		"next": "15.5.9",
		"react": "19.1.4",
		"react-dom": "19.1.4",
		"ws": "^8.19.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/ws": "^8.18.1",
		"dotenv": "^17.2.3",
		"dotenv-cli": "^11.0.0",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"prisma": "6.16.0",
		"tailwindcss": "^4",
		"tsx": "^4.21.0",
		"typescript": "^5",
		"wrangler": "^4.53.0"
	}
}
