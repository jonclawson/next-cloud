{
	"name": "app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"docker": "docker-compose up -d; docker exec -it nextcloud bash; docker-compose down",
    "exec": "docker exec -it nextcloud bash",
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"buildopen": "npx prisma generate && opennextjs-cloudflare build",
		"deploy": "opennextjs-cloudflare deploy",
		"upload": "opennextjs-cloudflare upload",
		"preview": "opennextjs-cloudflare preview --ip 0.0.0.0 --port 3000",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"postinstall": "prisma generate",
		"generate": "dotenv -e .dev.vars -- npx prisma generate --no-engine"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^2.11.1",
		"@opennextjs/cloudflare": "^1.14.4",
		"@prisma/adapter-pg": "^7.2.0",
		"@prisma/client": "7.2.0",
		"bcryptjs": "^3.0.3",
		"next": "15.5.9",
		"next-auth": "^5.0.0-beta.30",
		"react": "19.1.4",
		"react-dom": "19.1.4"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/bcryptjs": "^2.4.6",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/ws": "^8.18.1",
		"dotenv": "^17.2.3",
		"dotenv-cli": "^11.0.0",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"prisma": "7.2.0",
		"tailwindcss": "^4",
		"tsx": "^4.21.0",
		"typescript": "^5",
		"wrangler": "^4.53.0"
	}
}
