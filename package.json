{
	"name": "app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"docker": "docker-compose up -d; docker exec -it nextcloud bash; docker-compose down",
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"buildopen": "opennextjs-cloudflare build",
		"deploy": "opennextjs-cloudflare deploy",
		"upload": "opennextjs-cloudflare upload",
		"preview": "opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:seed": "dotenv -e .dev.vars -- tsx src/lib/db/seed.ts"
	},
	"dependencies": {
		"@neondatabase/serverless": "^1.0.2",
		"@opennextjs/cloudflare": "^1.14.4",
		"drizzle-orm": "^0.45.1",
		"next": "15.5.9",
		"react": "19.1.4",
		"react-dom": "19.1.4",
		"ws": "^8.19.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/ws": "^8.18.1",
		"dotenv": "^17.2.3",
		"dotenv-cli": "^11.0.0",
		"drizzle-kit": "^0.29.1",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"tailwindcss": "^4",
		"tsx": "^4.21.0",
		"typescript": "^5",
		"wrangler": "^4.53.0"
	}
